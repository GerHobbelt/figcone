<root rootDir="~/Photos"
      supportedFiles = "[ '.jpg', 'png']">
    <thumbnails enabled = "1"
                maxWidth = "128"
                maxHeight = "128" />
    <sharedAlbums>
        <album dir="summer_2019"
               name="Summer (2019)">
            <hosts>
                <host ip="127.0.0.1"
                      port="8080"/>
            </hosts>
        </album>
        <album dir="misc"
               name="Misc"/>
    </sharedAlbums>
  <envVars DISPLAY = "0.1"/>
</root>